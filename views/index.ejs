<%- include("header") %>

<div class="main_center">
	<div class="main_slider">
		<section id="center">

			<div class="center_1">
				<div class="carousel fade-carousel slide" data-ride="carousel" data-interval="4000" id="bs-carousel">
					<!-- Overlay -->
					<div class="overlay"></div>

					<!-- Indicators -->
					<ol class="carousel-indicators">
						<li data-target="#bs-carousel" data-slide-to="0" class="active"></li>
						<li data-target="#bs-carousel" data-slide-to="1" class=""></li>
						<li data-target="#bs-carousel" data-slide-to="2" class=""></li>
					</ol>

					<!-- Wrapper for slides -->

					<div class="carousel-inner">
						<div class="item slides active">
							<div class="slide-1"></div>
							<div class="hero">
								<div class="col-sm-6"> </div>
								<div class="col-sm-6">
									<hgroup>
										<h1>WE TRAVEL</h1>
										<h3>not to escape life.</h3>
										<h1>BUT FOR LIFE</h1>
										<h3>not to escape us.</h3>
									</hgroup>
									<button class="btn btn-hero btn-lg" role="button">Trip Now</button>
								</div>
							</div>
						</div>
						<div class="item slides">
							<div class="slide-2"></div>
							<div class="hero">
								<div class="col-sm-6"> </div>
								<div class="col-sm-6">
									<hgroup>
										<h1>TRAVEL IS NEVER A MATTER OF MONEY</h1>
										<h3>but of courage.</h3>
									</hgroup>
									<button class="btn btn-hero btn-lg" role="button">Trip Now</button>
								</div>
							</div>
						</div>
						<div class="item slides">
							<div class="slide-3"></div>
							<div class="hero">
								<div class="col-sm-6"></div>
								<div class="col-sm-6">
									<hgroup>
										<h1>COVER THE EARCH</h1>
										<h3>BEFORE IT COVERS YOU.</h3>
									</hgroup>
									<button class="btn btn-hero btn-lg" role="button">Trip Now</button>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>

		</section>
	</div>
</div>

<section id="content">
	<div class="container">
		<div class="row">
			<div class="col-sm-4 content_1">
				<p><i class="fa fa-check-circle"></i>Allow you to meet new people.</p>
			</div>
			<div class="col-sm-4 content_1">
				<p><i class="fa fa-check-circle"></i>Gives you the chance to try new things.</p>
			</div>
			<div class="col-sm-4 content_1">
				<p><i class="fa fa-check-circle"></i>Let's you experience other cultures.</p>
			</div>
		</div>
	</div>
</section>

<section id="routes">
	<div class="container">
		<div class="row">
			<div class="col-sm-12">
				<% for (const home of model) {%>
				<div class="col-sm-3">
					<div class="routes_inner">
					 <a href="/domestic/<%= home.id %>"><img src="<%= home.image_one %>" width="100%"></a>
					  <div class="routes_inner_1 clearfix">
						 <h4><%=home.title %></h4>
						 <h6><%= home.place %></h6>
						 <h5><%= home.trip_validty %></h5>
						 <h6><%= home.trip_period %></h6>
						 <span> <a href="domestic/<%= home.id %>">View Ways</a></span>
						 <!-- <span class="booking_1"> <a href="#">Book Now</a></span> -->
					   </div>
					</div>
				</div>
				<%} %>
			</div>
		</div>
	</div>
</section>



